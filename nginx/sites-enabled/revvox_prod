server {
    listen 443;
    server_name prod.revvox;
		access_log logs/access/prod_revvox.log;
    error_log logs/error/prod_revvox.log debug;

    location / {
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header Host $host;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
				proxy_pass https://localhost:543;
    }
}
