server {
    server_name prod.de.tbs.toys;
		access_log logs/access/prod_de_tbs_toys.log;
    error_log logs/error/prod_de_tbs_toys.log;

    location / {
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header Host $host;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
				proxy_pass https://localhost:543;
    }
}
